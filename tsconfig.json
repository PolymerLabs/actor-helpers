{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",

    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,

    "lib": [
      "dom",
      "webworker",
      "es6"
    ],
    "outDir": "./lib",
    "baseUrl": "./",
    "skipLibCheck": true,

    "declaration": true,
    "sourceMap": true,
    "pretty": true
  }
}
